{
  "name": "ikt205finalproject",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "type": "module",
  "engines": {
    "node": "19.4.0",
    "npm": "9.2.0"
  },
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "ts-node src/server.ts",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "dev": "nodemon --exec ts-node src/server.ts"
  },
  "compilerOptions": {
    "module": "CommonJS"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|expo-status-bar|react-native-chart-kit|react-native)"
    ],
    "globals": {
      "__DEV__": true
    },
    "moduleFileExtensions": [
      "js",
      "mjs",
      "ts",
      "tsx",
      "jsx",
      "json",
      "node",
      "android.js",
      "ios.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "./node_modules/react-native/jest/setup.js"
    ]
  },
  "dependencies": {
    "@types/stripe": "^8.0.417",
    "babel-jest": "^29.4.3",
    "bcrypt": "^5.1.0",
    "expo": "~48.0.4",
    "expo-status-bar": "~1.4.4",
    "express": "^4.18.2",
    "firebase": "^9.17.2",
    "helmet": "^6.0.1",
    "jest": "^29.4.3",
    "jest-expo": "^48.0.1",
    "mongoose": "^7.0.0",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-test-renderer": "^18.2.0",
    "stripe": "^11.14.0",
    "ts-jest": "^29.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "@types/react": "~18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katsiarynl/-IKT205ExamProjectBackend.git"
  },
  "keywords": [],
