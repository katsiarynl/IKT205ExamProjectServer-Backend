{
  "name": "ikt205finalproject",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "type": "module",
  "engines": {
    "node": "19.4.0",
    "npm": "9.6.4"
  },
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "ts-node src/server.ts",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --runInBand --force-exit",
    "dev": "nodemon --exec ts-node src/server.ts"
  },
  "compilerOptions": {
    "module": "CommonJS"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native)"
    ],
    "globals": {
      "__DEV__": true
    },
    "moduleFileExtensions": [
      "js",
      "mjs",
      "ts",
      "tsx",
      "jsx",
      "json",
      "node",
      "android.js",
      "ios.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "dependencies": {
    "babel-jest": "^29.4.3",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "expo": "~48.0.4",
    "expo-status-bar": "~1.4.4",
    "express": "^4.18.2",
    "firebase": "^9.19.1",
    "firebase-admin": "^11.6.0",
    "helmet": "^6.1.5",
    "jest-expo": "^48.0.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "mongoose": "^7.0.3",
    "nodemailer": "^6.9.1",
    "stripe": "^11.18.0",
    "ts-jest": "^29.0.5",
    "@types/react": "~18.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "@types/stripe": "^8.0.417",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "superagent": "^8.0.9",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katsiarynl/-IKT205ExamProjectBackend.git"
  },
